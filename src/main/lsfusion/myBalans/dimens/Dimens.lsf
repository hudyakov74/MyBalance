                       pos BACKGROUND backgroundColor(d)
                       ON CONTEXTMENU 'Перенумеровать по иерархии от ...' {INPUT   inp = pos(d) DO {
                                                                               LOCAL n_ = INTEGER();
                                                                               n_() <- inp;
                                                                               FOR isParent(ObjectDim d_, d) ORDER canonicalName(d_)  DO {
                                                                                   pos(d_) <- n_();
                                                                                   n_() <- n_() + 2;
                                                                               }
                                                                           }
                                                                          }, 
                       code BACKGROUND backgroundColor(d), 
                       tabName BACKGROUND backgroundColor(d) STICKY, 
                       name BACKGROUND backgroundColor(d) 
       PROPERTIES      itemsName 'Имя для формул' = name(itemsName(d)),
                        parent 'Родитель' = name(parent(d)) ON CHANGE {DIALOG form##ObjectDim##TreeList  OBJECTS t = parent(d)  CHANGE = parent(d) LIST name(t) FILTERS isGroup(t); },
                         //selectGroup(d) BACKGROUND backgroundColor(d),
                       nameOrderFull(d) BACKGROUND backgroundColor(d),
                       uuidInit(d),
           name0L(d), name0LOrderFull (d), name1L(d), name1LOrderFull (d), name2L(d), name2LOrderFull (d), name3L(d), name3LOrderFull (d), name4L(d), name4LOrderFull(d)

       FILTERGROUP dubles FILTER 'Показать дубли' countDuble##ObjectDim(name(d)) > 1
       ORDERS  pos(d), nameOrderFull(d), isGroup(d) DESC 
       ; 
       DESIGN  form##ObjectDim##list {
           PROPERTY (name(d))     {charWidth=20;}
           PROPERTY (nameOrderFull(d))     {charWidth=2;}
           PROPERTY (tabName(d))  {charWidth=20; charHeight=1;}
           PROPERTY (parent)      {charWidth=20;}
           PROPERTY (uuidInit(d)) {charWidth=10;}
           PROPERTY (name0L(d)) {hide=TRUE;}
           PROPERTY (name0LOrderFull(d)) {hide=TRUE;}
           PROPERTY (name1L(d)) {hide=TRUE;}
           PROPERTY (name1LOrderFull(d)) {hide=TRUE;}           
           PROPERTY (name2L(d)) {hide=TRUE;}
           PROPERTY (name2LOrderFull(d)) {hide=TRUE;}           
           PROPERTY (name3L(d)) {hide=TRUE;}
           PROPERTY (name3LOrderFull(d)) {hide=TRUE;}           
           PROPERTY (name4L(d)) {hide=TRUE;}
           PROPERTY (name4LOrderFull(d)) {hide=TRUE;}
       }  
       NAVIGATOR {
                   dimCatalogs   {
                    NEW form##ObjectDim##list;
                   }
                 }
        
delAll##ObjectDim 'удалить видимое'() {
