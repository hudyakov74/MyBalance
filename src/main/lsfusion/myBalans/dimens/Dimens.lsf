FORM listBreportSelected###ObjectDim 'Список'  
    OBJECTS c = BReportPartitionFilter PANEL
    TREE   groupTree g = ObjectDim PARENT parent(g)
    OBJECTS          s = ObjectDim,
                  sSel 'ВЫБРАНО' = ObjectDim
      
    PROPERTIES READONLY nameG = IF isRoot(g) THEN ' ВСЁ' ELSE name(g)
    ORDERS nameG
    FILTERS isGroup(g) OR isRoot(g)
    
    PROPERTIES  BACKGROUND backgroundColor(s)
       lBrepSel(s, c) DRAW s GRID,
       nameS 'Наименование' = name(s) READONLY,
       isGroup(s)
    ORDERS  isGroup(s) DESC, nameS
    FILTERS isRoot(g) OR isParent(s, g)
    //FILTERS NOT isGroup(s)
    
    PROPERTIES BACKGROUND backgroundColor(sSel)
       lBrepSel(sSel, c) DRAW sSel GRID,
       name  'Наименование' = name(sSel) READONLY
    ORDERS name
    FILTERS lBrepSel(sSel, c)  
    
    ON INIT {
        SEEK listBreportSelected###ObjectDim.s = sSel;
    }
;

